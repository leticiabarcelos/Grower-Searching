{
  "name": "crud-grower",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "description": "Um crud com Vue.js e json-server",
  "author": "LetÃ­cia Barcelos",
  "scripts": {
    "watchify": "watchify -vd -p browserify-hmr -e src/main.js -o dist/build.js",
    "serve": "json-server --watch grower.json --static ./ --port 8080",
    "dev": "npm-run-all --parallel watchify serve",
    "build": "cross-env NODE_ENV=production browserify src/main.js | uglifyjs -c warnings=false -m > dist/build.js"
  },
  "dependencies": {
    "bulma": "^0.1.0",
    "font-awesome": "^4.6.3",
    "lodash.sortby": "^4.7.0",
    "sweetalert": "^1.1.3",
    "vue": "^1.0.0",
    "vue-bootstrap-pagination": "^2.10.3",
    "vue-ionicons": "^3.0.4",
    "vue-lodash": "^2.0.2",
    "vue-resource": "^0.9.3",
    "vuejs-paginate": "^2.1.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^6.0.0",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "browserify-hmr": "^0.3.1",
    "cross-env": "^1.0.6",
    "http-server": "^0.11.1",
    "json-server": "^0.15.0",
    "node-sass": "^7.0.0",
    "npm-run-all": "^1.6.0",
    "uglify-js": "^2.5.0",
    "vueify": "^8.5.2",
    "watchify": "^3.4.0"
